{
    "name": "linio/safeecho",
    "type": "library",
    "description": "Lightweight function to easily hide printed data.",
    "keywords": ["linio", "echo", "print", "secure", "safeecho", "safe echo", "safe print", "password", "encrypt"],
    "license": "MIT",
    "authors": [
        {
            "name": "Melissa Billias",
            "role": "Developer"
        }
    ],
    "require": {
        "php": ">=5.3.0",
        "hassankhan/config": "^0.10.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^5.4",
        "friendsofphp/php-cs-fixer": "^2.0",
        "phpunit/php-code-coverage": "^4"
    },
    "config": {
        "bin-dir": "bin"
    },
    "autoload": {
        "psr-4": {
            "Linio\\SafeEcho\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Linio\\SafeEcho\\tests\\": "tests/"
        }
    }
}
